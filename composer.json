{
    "name": "starring-jane/wordpress-blade",
    "description": "A composer package that enables Wordpress to use the Blade templating engine",
    "type": "library",
    "license": "MIT",
    "authors": [{
            "name": "Laurens Bultynck",
            "email": "laurens@starringjane.com"
        },
        {
            "name": "Maxim Vanhove",
            "email": "maxim@starringjane.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "jenssegers/blade": "^1.3",
        "tightenco/collect": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "StarringJane\\WordpressBlade\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/helpers.php"
        ],
        "psr-4": {
            "StarringJane\\WordpressBlade\\Tests\\": "tests"
        }
    }
}
